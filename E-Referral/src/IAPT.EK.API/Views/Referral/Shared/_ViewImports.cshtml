@namespace IAPT.EK.API.Views.Referral
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IAPT.EK.Business.EReferral.Models.eAgencyInformation


<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Agency Information</h3>
    </div>
    <div class="panel-body">
        <table class="table table-striped">
            <tbody>
                <tr>
                    <td>Agency:</td>
                    <td>@Model?.Agency</td>
                </tr>
                <tr>
                    <td>Contact Person:</td>
                    <td>@Model?.ContactPerson</td>
                </tr>
                <tr>
                    <td>Phone:</td>
                    <td>@Model?.Phone</td>
                </tr>
                <tr>
                    <td>email:</td>
                    <td>@Model?.Email</td>
                </tr>
                <tr>
                    <td>Agency Has Client Consent:</td>
                    <td>@Model?.AgencyHasClientConsent</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

