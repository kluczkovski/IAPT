@model IAPT.EK.Business.EReferral.Models.eReferral

<!DOCTYPE html>

<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="~/ccs/referralStyle.css" />
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css"
          integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">

    <!-- Optional theme -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap-theme.min.css"
          integrity="sha384-6pzBo3FDv/PJ8r2KRkGHifhEocL+1X2rVCTTkUfGk7/0pbek5mMa1upzvWbrUbOZ" crossorigin="anonymous">

    <!-- Latest compiled and minified JavaScript -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"
            integrity="sha384-aJ21OjlMXNL5UyIl/XNwTMqvzeRMZH2w8c5cRVpzpU8Y5bApTppSuUkhZXN0VxHd" crossorigin="anonymous"></script>
   
</head>

<body class="container">

    @* Header of Report *@
    @await Html.PartialAsync("_00_Header")

    <div class="text-center">
        <p><b style="font-size:36px">Support Referral Form</b> <sub>(v20/01)</sub></p>
    </div>

    @* Agency Information *@
    @await Html.PartialAsync("_10_Agency", Model.eAgencyInformation)

    @* Contact Detail *@
    @await Html.PartialAsync("_20_ContactDetail", Model.eContactDetail)

    @* Diversity Information *@
    @await Html.PartialAsync("_30_DiversityIndicator", Model.eDiversity)

    @* Armed Forces Indicator *@
    @await Html.PartialAsync("_40_ArmedForceIndicator", Model.eArmedForce)

    @* Long Term Physical Health Indicator *@
    @await Html.PartialAsync("_50_LongTermPhysicalHealthIndicator", Model.eLongTermPhysicalHealth)

    @* Risk Indicator *@
    @await Html.PartialAsync("_60_RiskIndicator", Model.eRiskIndicator)

    @* Clinical Referral Information *@
    @await Html.PartialAsync("_70_ClinicalReferralInfo", Model.EClinicalReferralInfo)


    @* Criminal Justice Support Referral Information *@
    @await Html.PartialAsync("_80_CriminalJusticeReferralInfo", Model.eIsvaReferralInfo)

    @* Referral Information *@
    @await Html.PartialAsync("_90_ReferralInformation", Model.eSexualOffence)

    @* Common Survivor Impact Indicators *@
    @await Html.PartialAsync("_95_CommonSurvivorsImpactIndicator", Model.eCommonSurvivorImpact)

    @*@await Html.PartialAsync("_XX_Test", Model.eSexualOffence)

    @await Html.PartialAsync("_XY_Test", Model.eSexualOffence)*@

</body>

</html>